@prefix : <http://www.semanticweb.org/jessanrod3/ontologies/2022/2/Series#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://www.semanticweb.org/jessanrod3/ontologies/2022/2/Series#> .

<http://www.semanticweb.org/jessanrod3/ontologies/2022/2/Series> rdf:type owl:Ontology ;
                                                                  owl:versionIRI <http://www.semanticweb.org/jessanrod3/ontologies/2022/2/Series/1.0> ;
                                                                  <http://purl.org/dc/terms/license> "Version 1.0"@en ;
                                                                  rdfs:comment """Paquita Salas Ontology
Created by Jesús Sánchez Rodríguez"""@en .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/terms/license
<http://purl.org/dc/terms/license> rdf:type owl:AnnotationProperty .


###  http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled
<http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled> rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2000/01/rdf-schema#sameAs
rdfs:sameAs rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2002/07/owl#versionIRI
owl:versionIRI rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://www.semanticweb.org/jessanrod3/ontologies/2022/2/Series#appearsIn
:appearsIn rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf owl:topObjectProperty ;
           rdfs:domain [ rdf:type owl:Class ;
                         owl:unionOf ( :Character
                                       :Person
                                     )
                       ] ;
           rdfs:range [ rdf:type owl:Class ;
                        owl:unionOf ( :Season
                                      :Series
                                    )
                      ] ;
           rdfs:comment "Describes an object property between a character and a season"@en .


###  http://www.semanticweb.org/jessanrod3/ontologies/2022/2/Series#hasBeenWonBy
:hasBeenWonBy rdf:type owl:ObjectProperty ;
              owl:inverseOf :hasWon ;
              rdfs:domain :Award ;
              rdfs:range [ rdf:type owl:Class ;
                           owl:unionOf ( :Person
                                         :Series
                                       )
                         ] .


###  http://www.semanticweb.org/jessanrod3/ontologies/2022/2/Series#hasWon
:hasWon rdf:type owl:ObjectProperty ;
        rdfs:subPropertyOf owl:topObjectProperty ;
        rdfs:domain [ rdf:type owl:Class ;
                      owl:unionOf ( :Person
                                    :Series
                                  )
                    ] ;
        rdfs:range :Award ;
        rdfs:comment "Describes an object property between a series or a person and an award"@en .


###  http://www.semanticweb.org/jessanrod3/ontologies/2022/2/Series#interprets
:interprets rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf owl:topObjectProperty ;
            rdfs:domain :Actor ;
            rdfs:range :Character ;
            rdfs:comment "Describes an object property between an actor and a character"@en .


###  http://www.semanticweb.org/jessanrod3/ontologies/2022/2/Series#sings
:sings rdf:type owl:ObjectProperty ,
                owl:IrreflexiveProperty ;
       rdfs:domain [ rdf:type owl:Class ;
                     owl:unionOf ( :Character
                                   :Singer
                                 )
                   ] ;
       rdfs:range :Song ;
       rdfs:comment "Describes an object property between a singer o character and a song sung by him/her"@en .


#################################################################
#    Data properties
#################################################################

###  http://www.semanticweb.org/jessanrod3/ontologies/2022/2/Series#hasAward
:hasAward rdf:type owl:DatatypeProperty ;
          rdfs:domain [ rdf:type owl:Class ;
                        owl:unionOf ( :Person
                                      :Series
                                    )
                      ] ;
          rdfs:range xsd:boolean .


###  http://www.semanticweb.org/jessanrod3/ontologies/2022/2/Series#interpretsThemselves
:interpretsThemselves rdf:type owl:DatatypeProperty ;
                      rdfs:domain :Actor ;
                      rdfs:range xsd:boolean .


###  http://www.semanticweb.org/jessanrod3/ontologies/2022/2/Series#numberOfEpisodes
:numberOfEpisodes rdf:type owl:DatatypeProperty ;
                  rdfs:subPropertyOf owl:topDataProperty ;
                  rdf:type owl:FunctionalProperty ;
                  rdfs:domain [ rdf:type owl:Class ;
                                owl:unionOf ( :Character
                                              :Season
                                              :Series
                                            )
                              ] ;
                  rdfs:range xsd:nonNegativeInteger ;
                  rdfs:comment "Number of episodes of a season"@en ;
                  rdfs:sameAs "http://dbpedia.org/ontology/numberOfEpisodes" .


###  http://www.semanticweb.org/jessanrod3/ontologies/2022/2/Series#soundtrackSong
:soundtrackSong rdf:type owl:DatatypeProperty ;
                rdfs:domain :Song ;
                rdfs:range xsd:boolean .


#################################################################
#    Classes
#################################################################

###  http://www.semanticweb.org/jessanrod3/ontologies/2022/2/Series#ActingAward
:ActingAward rdf:type owl:Class ;
             rdfs:subClassOf :Award ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :hasBeenWonBy ;
                               owl:allValuesFrom :Actor
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :hasBeenWonBy ;
                               owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                               owl:onClass :Actor
                             ] ;
             owl:disjointWith :SeriesAward ;
             rdfs:comment "Awards related to actors"@en .


###  http://www.semanticweb.org/jessanrod3/ontologies/2022/2/Series#Actor
:Actor rdf:type owl:Class ;
       rdfs:subClassOf :Person ;
       rdfs:comment "This classe represents an actor or actress" ;
       rdfs:label "Actor"@en ;
       rdfs:sameAs "https://dbpedia.org/ontology/Actor"@en .


###  http://www.semanticweb.org/jessanrod3/ontologies/2022/2/Series#Award
:Award rdf:type owl:Class ;
       rdfs:subClassOf owl:Thing ;
       rdfs:comment "Awards won (or nominated to) by a person or series"@en ;
       rdfs:label "Award"@en ;
       rdfs:sameAs "https://dbpedia.org/ontology/Award"@en .


###  http://www.semanticweb.org/jessanrod3/ontologies/2022/2/Series#BestComedySeriesAward
:BestComedySeriesAward rdf:type owl:Class ;
                       rdfs:subClassOf :SeriesAward ;
                       rdfs:comment "Award related to the best comedy series"@en .


###  http://www.semanticweb.org/jessanrod3/ontologies/2022/2/Series#BestMainActorAward
:BestMainActorAward rdf:type owl:Class ;
                    rdfs:subClassOf :ActingAward ;
                    owl:disjointWith :BestSupportingActressAward ;
                    rdfs:comment "Award related to the best main male actor"@en .


###  http://www.semanticweb.org/jessanrod3/ontologies/2022/2/Series#BestMainActressAward
:BestMainActressAward rdf:type owl:Class ;
                      rdfs:subClassOf :ActingAward ;
                      rdfs:comment "Award related to the best main female actor"@en .


###  http://www.semanticweb.org/jessanrod3/ontologies/2022/2/Series#BestSupportingActorAward
:BestSupportingActorAward rdf:type owl:Class ;
                          rdfs:subClassOf :ActingAward ;
                          rdfs:comment "Award related to the best supporting actor"@en .


###  http://www.semanticweb.org/jessanrod3/ontologies/2022/2/Series#BestSupportingActressAward
:BestSupportingActressAward rdf:type owl:Class ;
                            rdfs:subClassOf :ActingAward ;
                            rdfs:comment "Award related to the best supporting actress"@en .


###  http://www.semanticweb.org/jessanrod3/ontologies/2022/2/Series#Character
:Character rdf:type owl:Class ;
           rdfs:comment "This class represents a character in the series"@en ;
           rdfs:label "Character"@en ;
           rdfs:sameAs "https://dbpedia.org/ontology/FictionalCharacter"@en .


###  http://www.semanticweb.org/jessanrod3/ontologies/2022/2/Series#Creator
:Creator rdf:type owl:Class ;
         rdfs:subClassOf :Person ;
         rdfs:comment "This class represents a series creator" .


###  http://www.semanticweb.org/jessanrod3/ontologies/2022/2/Series#Director
:Director rdf:type owl:Class ;
          rdfs:subClassOf :Person ;
          rdfs:comment "This class represents a series director" .


###  http://www.semanticweb.org/jessanrod3/ontologies/2022/2/Series#GuestCharacter
:GuestCharacter rdf:type owl:Class ;
                rdfs:subClassOf :Character ;
                rdfs:comment "This class represents a guest character"@en .


###  http://www.semanticweb.org/jessanrod3/ontologies/2022/2/Series#MainCharacter
:MainCharacter rdf:type owl:Class ;
               rdfs:subClassOf :Character ;
               rdfs:comment "This class represents the main character(s)"@en .


###  http://www.semanticweb.org/jessanrod3/ontologies/2022/2/Series#Man
:Man rdf:type owl:Class ;
     rdfs:subClassOf :Person ;
     owl:disjointWith :Woman ;
     rdfs:comment "This class represents a male human"@en ;
     rdfs:sameAs "https://dbpedia.org/ontology/Man"@en .


###  http://www.semanticweb.org/jessanrod3/ontologies/2022/2/Series#Person
:Person rdf:type owl:Class ;
        rdfs:comment "This class represents a real person" ;
        rdfs:label "Person"@en ;
        rdfs:sameAs "https://dbpedia.org/ontology/Person"@en .


###  http://www.semanticweb.org/jessanrod3/ontologies/2022/2/Series#RecurrentCharacter
:RecurrentCharacter rdf:type owl:Class ;
                    rdfs:subClassOf :Character ;
                    rdfs:comment "This class represents current characters"@en .


###  http://www.semanticweb.org/jessanrod3/ontologies/2022/2/Series#Season
:Season rdf:type owl:Class ;
        rdfs:comment "This class represents a series' season"@en .


###  http://www.semanticweb.org/jessanrod3/ontologies/2022/2/Series#Series
:Series rdf:type owl:Class ;
        rdfs:comment "This class denotes the serie"@en ;
        rdfs:label "Series"@en ;
        rdfs:sameAs "http://dbpedia.org/ontology/TelevisionShow"@en .


###  http://www.semanticweb.org/jessanrod3/ontologies/2022/2/Series#SeriesAward
:SeriesAward rdf:type owl:Class ;
             rdfs:subClassOf :Award ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :hasBeenWonBy ;
                               owl:allValuesFrom :Series
                             ] ;
             rdfs:comment "Award related to a series"@en .


###  http://www.semanticweb.org/jessanrod3/ontologies/2022/2/Series#Singer
:Singer rdf:type owl:Class ;
        rdfs:subClassOf :Person ;
        rdfs:comment "Singer of series' main song" ;
        rdfs:sameAs "https://dbpedia.org/ontology/Singer" .


###  http://www.semanticweb.org/jessanrod3/ontologies/2022/2/Series#Song
:Song rdf:type owl:Class ;
      rdfs:comment "Songs in the series"@en ;
      rdfs:sameAs "https://dbpedia.org/ontology/Song" .


###  http://www.semanticweb.org/jessanrod3/ontologies/2022/2/Series#Woman
:Woman rdf:type owl:Class ;
       rdfs:subClassOf :Person ;
       rdfs:comment "This class represents a female human"@en ;
       rdfs:sameAs "https://dbpedia.org/ontology/Woman"@en .



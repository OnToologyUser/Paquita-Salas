#################################################################
#    Rules
#################################################################

:x rdf:type <http://www.w3.org/2003/11/swrl#Variable> .

:y rdf:type <http://www.w3.org/2003/11/swrl#Variable> .

<urn:swrl:var#x> rdf:type <http://www.w3.org/2003/11/swrl#Variable> .

<urn:swrl:var#y> rdf:type <http://www.w3.org/2003/11/swrl#Variable> .

<urn:swrl:var#z> rdf:type <http://www.w3.org/2003/11/swrl#Variable> .

[ <http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled> "true"^^xsd:boolean ;
   rdfs:comment "" ;
   rdfs:label "S4" ;
   rdf:type <http://www.w3.org/2003/11/swrl#Imp> ;
   <http://www.w3.org/2003/11/swrl#body> [ rdf:type <http://www.w3.org/2003/11/swrl#AtomList> ;
                                           rdf:first [ rdf:type <http://www.w3.org/2003/11/swrl#ClassAtom> ;
                                                       <http://www.w3.org/2003/11/swrl#classPredicate> :Singer ;
                                                       <http://www.w3.org/2003/11/swrl#argument1> :x
                                                     ] ;
                                           rdf:rest [ rdf:type <http://www.w3.org/2003/11/swrl#AtomList> ;
                                                      rdf:first [ rdf:type <http://www.w3.org/2003/11/swrl#ClassAtom> ;
                                                                  <http://www.w3.org/2003/11/swrl#classPredicate> :Song ;
                                                                  <http://www.w3.org/2003/11/swrl#argument1> :y
                                                                ] ;
                                                      rdf:rest [ rdf:type <http://www.w3.org/2003/11/swrl#AtomList> ;
                                                                 rdf:first [ rdf:type <http://www.w3.org/2003/11/swrl#IndividualPropertyAtom> ;
                                                                             <http://www.w3.org/2003/11/swrl#propertyPredicate> :sings ;
                                                                             <http://www.w3.org/2003/11/swrl#argument1> :x ;
                                                                             <http://www.w3.org/2003/11/swrl#argument2> :y
                                                                           ] ;
                                                                 rdf:rest rdf:nil
                                                               ]
                                                    ]
                                         ] ;
   <http://www.w3.org/2003/11/swrl#head> [ rdf:type <http://www.w3.org/2003/11/swrl#AtomList> ;
                                           rdf:first [ rdf:type <http://www.w3.org/2003/11/swrl#DatavaluedPropertyAtom> ;
                                                       <http://www.w3.org/2003/11/swrl#propertyPredicate> :soundtrackSong ;
                                                       <http://www.w3.org/2003/11/swrl#argument1> :y ;
                                                       <http://www.w3.org/2003/11/swrl#argument2> "true"^^xsd:boolean
                                                     ] ;
                                           rdf:rest rdf:nil
                                         ]
 ] .

[ rdfs:comment "If an actor interprets a character and the character appears in a season, then the actor appears in the season"@en ;
   rdf:type <http://www.w3.org/2003/11/swrl#Imp> ;
   <http://www.w3.org/2003/11/swrl#body> [ rdf:type <http://www.w3.org/2003/11/swrl#AtomList> ;
                                           rdf:first [ rdf:type <http://www.w3.org/2003/11/swrl#ClassAtom> ;
                                                       <http://www.w3.org/2003/11/swrl#classPredicate> :Actor ;
                                                       <http://www.w3.org/2003/11/swrl#argument1> <urn:swrl:var#x>
                                                     ] ;
                                           rdf:rest [ rdf:type <http://www.w3.org/2003/11/swrl#AtomList> ;
                                                      rdf:first [ rdf:type <http://www.w3.org/2003/11/swrl#ClassAtom> ;
                                                                  <http://www.w3.org/2003/11/swrl#classPredicate> :Character ;
                                                                  <http://www.w3.org/2003/11/swrl#argument1> <urn:swrl:var#y>
                                                                ] ;
                                                      rdf:rest [ rdf:type <http://www.w3.org/2003/11/swrl#AtomList> ;
                                                                 rdf:first [ rdf:type <http://www.w3.org/2003/11/swrl#IndividualPropertyAtom> ;
                                                                             <http://www.w3.org/2003/11/swrl#propertyPredicate> :interprets ;
                                                                             <http://www.w3.org/2003/11/swrl#argument1> <urn:swrl:var#x> ;
                                                                             <http://www.w3.org/2003/11/swrl#argument2> <urn:swrl:var#y>
                                                                           ] ;
                                                                 rdf:rest [ rdf:type <http://www.w3.org/2003/11/swrl#AtomList> ;
                                                                            rdf:first [ rdf:type <http://www.w3.org/2003/11/swrl#IndividualPropertyAtom> ;
                                                                                        <http://www.w3.org/2003/11/swrl#propertyPredicate> :appearsIn ;
                                                                                        <http://www.w3.org/2003/11/swrl#argument1> <urn:swrl:var#y> ;
                                                                                        <http://www.w3.org/2003/11/swrl#argument2> <urn:swrl:var#z>
